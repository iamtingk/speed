<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorSpeedLightSkyBlue</item>
        <item name="colorPrimaryDark">@color/colorSpeedDarkSkyBlue</item>
